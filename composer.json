{
    "name": "amethyst/morph-to-relation",
    "type": "library",
    "keywords": ["laravel", "amethyst", "morph-to-relation"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.*",
        "orchestra/testbench": "*",
        "amethyst/relation": "0.2.*",
        "amethyst/foo": "0.2.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "morph-to-relation",
            "namespace": "Amethyst"
        },
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\MorphToRelationServiceProvider"
            ]
        }
    }
}
